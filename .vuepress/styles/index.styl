body 
    font-family 'Miriam Libre' !important

.home
    padding 3.6rem 0 0 !important
    max-width 100% !important

.home .hero
    display: none !important

.bg-coollabs
    background-color #2c3351

.text-coollabs
    color #8bd94f

.border-coollabs 
    border-bottom-color #8bd94f

.content:not(.custom)
    max-width none !important
    padding 0 !important

.navbar  
    border-bottom none !important
    box-shadow 0 1px 2px 0 black !important

.navbar .site-name
    font-weight 400 !important

.nav-link, .repo-link , .nav-links .dropdown-title
    font-weight 800 !important

aside.sidebar     
    background-color #2c3351 !important
    color white
    border-right none !important

nav.nav-links
    border-bottom 1px rgba(139, 217, 79, 0.1) solid !important

.nav-link
    text-transform uppercase !important
    font-weight 800 !important
